{
  "name": "medusa",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "build:verify": "rimraf .next-verify && cross-env NEXT_BUILD_DIR=.next-verify next build && rimraf .next-verify",
    "start": "next start",
    "lint": "biome lint --write .",
    "format": "biome format --write .",
    "prepare": "husky",
    "check:deps": "depcruise src",
    "check:types": "tsc --noEmit",
    "schema:update": "supabase gen types typescript --linked > src/types/database.types.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.943.0",
    "@date-fns/tz": "^1.4.1",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.84.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@tailwindcss/typography": "^0.5.19",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "browser-image-compression": "^2.0.2",
    "cheerio": "^1.1.2",
    "classnames": "^2.5.1",
    "d3": "^5.15.0",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.0",
    "exifr": "^7.1.3",
    "highcharts": "^12.4.0",
    "highcharts-react-official": "^3.2.3",
    "marked": "^17.0.1",
    "next": "16.1.2",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-icons": "^5.5.0",
    "react-toastify": "^11.0.5",
    "react-tooltip": "^5.30.0",
    "recharts": "^2.15.4",
    "sanitize-html": "^2.17.0",
    "supabase": "^2.62.5",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.11",
    "@types/d3": "^7.4.3",
    "@types/node": "^25.0.0",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@types/sanitize-html": "^2.16.0",
    "cross-env": "^10.0.0",
    "dependency-cruiser": "^17.0.0",
    "husky": "^9.1.7",
    "knip": "^5.71.0",
    "lint-staged": "^16.2.7",
    "react-scan": "^0.4.3",
    "rimraf": "^6.1.2",
    "sass": "^1.94.2",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": [
      "biome check --write ."
    ]
  },
  "overrides": {
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3"
  }
}
