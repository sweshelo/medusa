{
  "name": "medusa",
  "version": "0.9.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "build:verify": "rimraf .next-verify && cross-env NEXT_BUILD_DIR=.next-verify next build && rimraf .next-verify",
    "start": "next start",
    "lint": "next lint --fix",
    "format": "prettier --write .",
    "prepare": "husky",
    "check:deps": "depcruise src",
    "check:types": "tsc --noEmit",
    "schema:update": "supabase gen types typescript --linked > src/types/database.types.ts"
  },
  "dependencies": {
    "@date-fns/tz": "^1.4.1",
    "@supabase/supabase-js": "^2.84.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "cheerio": "^1.1.2",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "highcharts": "^12.4.0",
    "highcharts-react-official": "^3.2.3",
    "next": "^15.5.6",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-icons": "^5.5.0",
    "react-toastify": "^11.0.5",
    "react-tooltip": "^5.30.0",
    "recharts": "^2.15.4",
    "supabase": "^2.62.5"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.1",
    "@types/node": "^22.19.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.48.0",
    "@typescript-eslint/parser": "^8.48.0",
    "cross-env": "^7.0.3",
    "dependency-cruiser": "^16.10.4",
    "eslint": "^9.39.1",
    "eslint-config-next": "15.5.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.6.2",
    "rimraf": "^6.1.2",
    "sass": "^1.94.2",
    "sass-loader": "^16.0.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
