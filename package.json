    "dependency-cruiser": "^16.10.1",
    "eslint": "^9.25.1",
    "eslint-config-next": "15.4.5",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "husky": "^9.1.7",
    "lint-staged": "^16.0.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "sass": "^1.87.0",
    "sass-loader": "^16.0.5",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.8.3"
  },
  "volta": {
    "node": "23.11.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
