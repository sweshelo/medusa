{
  "name": "medusa",
  "version": "0.7.4",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "build:verify": "rimraf .next-verify && cross-env NEXT_BUILD_DIR=.next-verify next build && rimraf .next-verify",
    "start": "next start",
    "lint": "next lint --fix",
    "format": "prettier --write .",
    "prepare": "husky",
