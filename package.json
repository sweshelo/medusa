{
  "name": "medusa",
  "version": "0.12.4",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "build:verify": "rimraf .next-verify && cross-env NEXT_BUILD_DIR=.next-verify next build && rimraf .next-verify",
    "start": "next start",
    "lint": "biome lint --write .",
    "format": "biome format --write .",
    "prepare": "husky",
    "check:deps": "depcruise src",
    "check:types": "tsc --noEmit",
    "schema:update": "supabase gen types typescript --linked > src/types/database.types.ts"
  },
  "dependencies": {
    "@date-fns/tz": "^1.4.1",
    "@supabase/supabase-js": "^2.84.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "cheerio": "^1.1.2",
    "classnames": "^2.5.1",
    "d3": "^5.15.0",
    "date-fns": "^4.1.0",
    "dompurify": "^3.3.0",
    "highcharts": "^12.4.0",
    "highcharts-react-official": "^3.2.3",
    "next": "16.0.7",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-icons": "^5.5.0",
    "react-toastify": "^11.0.5",
    "react-tooltip": "^5.30.0",
    "recharts": "^2.15.4",
    "supabase": "^2.62.5",
    "sanitize-html": "^2.17.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@types/d3": "^7.4.3",
    "@types/node": "^22.19.1",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@types/sanitize-html": "^2.16.0",
    "cross-env": "^7.0.3",
    "dependency-cruiser": "^16.10.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "rimraf": "^6.1.2",
    "sass": "^1.94.2",
    "sass-loader": "^16.0.6",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md}": [
      "biome check --write ."
    ]
  },
  "overrides": {
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3"
  }
}
