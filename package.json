  "dependencies": {
    "@date-fns/tz": "^1.2.0",
    "@supabase/supabase-js": "^2.49.4",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "cheerio": "^1.0.0",
    "classnames": "^2.5.1",
    "date-fns": "^4.1.0",
    "highcharts": "^12.0.0",
    "highcharts-react-official": "^3.2.1",
    "next": "^15.3.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-toastify": "^11.0.5",
    "react-tooltip": "^5.28.1",
    "recharts": "^2.15.3",
    "supabase": "^2.22.6"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.8",
    "@types/node": "^22.15.3",
    "@types/react": "^19.1.2",
